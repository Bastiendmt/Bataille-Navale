def indicesVersCaseLn(indices):
    ligne=str(indices[0])
    colonne=str(indices[1])
    indices=ligne+colonne
    return (indices)

#print indicesVersCaseLn('K4')

def caseLnVersIndices(indices):
    ligne=str(indices[0])
    colonne=str(indices[1])
    indices=[ligne,colonne]
    return (indices)


